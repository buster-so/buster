{
    "version": 2,
    "buildCommand": "cd ../.. && pnpm install --frozen-lockfile && cd apps/server && pnpm run build:vercel",
    "outputDirectory": "dist",
    "installCommand": "cd ../.. && pnpm install --frozen-lockfile",
    "builds": [
      {
        "src": "dist/index.cjs",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "dist/index.cjs"
      }
    ],
    "functions": {
      "dist/index.cjs": {
        "maxDuration": 30
      }
    }
  }
  